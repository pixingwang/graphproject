<style type="text/css">
.col-md-0 {
	display: none;
}

.node {
	stroke: #222;
	stroke-width: 1.5px;
}

.node.B {
	fill: #ff0000;
}

.node.C {
	fill: #37ff00;
}

.node.P {
	fill: #ff68fb;
}

.node.Per {
	fill: #fff50d;
}

.node.Com {
	fill: #fff50d;
}

.node.Com.xiaodai {
	fill: #d94600;
}

.node.Com.dichan {
	fill: #8600ff;
}

.link {
	stroke: #999;
	stroke-opacity: .6; /*stroke-width: 1; IE 浏览器爆炸*/
	fill: none;
}

.link.DB {
	stroke: #8B0000;
}

.link.JY {
	stroke: #757575;
}

.link.DK {
	stroke: #1F7CF3;
}

.link.DY {
	stroke: #EEAD0E;
}

.link.ZDB {
	stroke: #20B2AA;
}

.link.GL {
	stroke: #7D26CD;
}

.text {
	fill: black;
	font-size: 8px; "
	text-anchor ": "middle";
}

.arc {
	fill: #d2d5da;
}

.link.shadow:hover {
